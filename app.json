{
  "icon": "./assets/loga-second.png",
  "expo": {
    "splash": {
      "backgroundColor": "#000826",
      "image": "./assets/laddar.png",
      "resizeMode": "contain"
    },

    "icon": "./assets/loga-second.png",
    "notification": {
      "color": "#311b92"
    },
    "UIBackgroundModes": ["location", "fetch"],
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera.",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "isAccessMediaLocationEnable": "true"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysPermission": "Allow $(PRODUCT_NAME) to use your location.",
          "isIosBackgroundLocationEnabled": "Allow $(PRODUCT_NAME) to use your location"
        }
      ],
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ]
    ],
    "name": "MapProject",
    "slug": "map",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",

    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "icon": "./assets/laddar.png",

      "supportsTablet": true,
      "bundleIdentifier": "com.mariah4ara.Map",
      "config": {
        "googleMapsApiKey": "AIzaSyAQunm5x4PRSf2rHbI_vbhQI2zAaBTKWNI"
      },
      "backgroundColor": "#1B274A",
      "UIBackgroundModes": ["location", "fetch"],
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow the app to access your location while you use it to find nearby stores and services.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow the app to access your location even when it's not in use, to provide personalized recommendations based on your location.",
        "NSLocationAlwaysUsageDescription": "Allow the app to access your location even when it's not in use, to provide personalized recommendations based on your location.",
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you."
      }
    },
    "permissions": ["LOCATION", "BACKGROUND_LOCATION"],
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1B274A"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.mariah4ara.Map"
    },
    "web": {},
    "extra": {
      "eas": {
        "projectId": "52a4c29a-4a9d-4d85-86f1-b4ed6ca2bfa2"
      }
    },
    "owner": "maryh4ara"
  }
}
